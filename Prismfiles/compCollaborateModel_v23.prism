//=======================================
module M1
ta:[0..2] init 1;      //to control the turn
sela:[-1..7] init -1;
goala: bool init false;
execa: bool init false;
av0a: bool init true;
av1a: bool init true;
av2a: bool init true;
av3a: bool init true;
av4a: bool init true;
av5a: bool init true;
av6a: bool init true;
av7a: bool init true;

// P1 moves:
//appx_rsy_sat - represents the satisfaction of performance violation
[r0a!] (ta=0) & (app0_rs0_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=0) & (ta'=1);
[r1a!] (ta=0) & (app0_rs1_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=1) & (ta'=1);
[r2a!] (ta=0) & (app0_rs2_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=2) & (ta'=1);
[r3a!] (ta=0) & (app0_rs3_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=3) & (ta'=1);
[r4a!] (ta=0) & (app0_rs4_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=4) & (ta'=1);
[r5a!] (ta=0) & (app0_rs5_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=5) & (ta'=1);
[r6a!] (ta=0) & (app0_rs6_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=6) & (ta'=1);
[r7a!] (ta=0) & (app0_rs7_sat_ml_all=false) & (goala=false) -> (goala'=true) & (sela'=7) & (ta'=1);
//terminate the process
[enda!] (ta=0) & (goala=true) -> (ta'=2);

// P2 moves:
[ch0a?] (ta=1) & (execa=false) & (sela=0) -> AVA_RS0:(av0a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS0:(av0a'=false) & (execa'=false) & (ta'=2);
[ch1a?] (ta=1) & (execa=false) & (sela=1) -> AVA_RS1:(av1a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS1:(av1a'=false) & (execa'=false) & (ta'=2);
[ch2a?] (ta=1) & (execa=false) & (sela=2) -> AVA_RS2:(av2a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS2:(av2a'=false) & (execa'=false) & (ta'=2);
[ch3a?] (ta=1) & (execa=false) & (sela=3) -> AVA_RS3:(av3a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS3:(av3a'=false) & (execa'=false) & (ta'=2);
[ch4a?] (ta=1) & (execa=false) & (sela=4) -> AVA_RS4:(av4a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS4:(av4a'=false) & (execa'=false) & (ta'=2);
[ch5a?] (ta=1) & (execa=false) & (sela=5) -> AVA_RS5:(av5a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS5:(av5a'=false) & (execa'=false) & (ta'=2);
[ch6a?] (ta=1) & (execa=false) & (sela=6) -> AVA_RS6:(av6a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS6:(av6a'=false) & (execa'=false) & (ta'=2);
[ch7a?] (ta=1) & (execa=false) & (sela=7) -> AVA_RS7:(av7a'=true) & (execa'=true) & (ta'=2) + 1-AVA_RS7:(av7a'=false) & (execa'=false) & (ta'=2);

//start the move
[be?] (ta=1) -> (ta'=2);
//[end?] (ta=2) -> (ta'=2);

//schedule the turn between subsystems
[] (ta=2) -> (ta'=0);

endmodule

