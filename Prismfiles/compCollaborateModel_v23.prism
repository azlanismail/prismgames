[r1b!] (tb=0) & (app1_rs1_sat_ml_all=false) & (goalb=false) -> (goalb'=true) & (selb'=1) & (tb'=1);
[r2b!] (tb=0) & (app1_rs2_sat_ml_all=false) & (goalb=false) -> (goalb'=true) & (selb'=2) & (tb'=1);
