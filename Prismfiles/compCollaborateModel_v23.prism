//terminate the process
[endb!] (tb=0) & (goalb=true) -> (tb'=2);

// P2 moves:
[ch0b?] (tb=1) & (execb=false) & (selb=0) -> AVB_RS0:(av0b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS0:(av0b'=false) & (execb'=false) & (tb'=2);
[ch1b?] (tb=1) & (execb=false) & (selb=1) -> AVB_RS1:(av1b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS1:(av1b'=false) & (execb'=false) & (tb'=2);
[ch2b?] (tb=1) & (execb=false) & (selb=2) -> AVB_RS2:(av2b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS2:(av2b'=false) & (execb'=false) & (tb'=2);
[ch3b?] (tb=1) & (execb=false) & (selb=3) -> AVB_RS3:(av3b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS3:(av3b'=false) & (execb'=false) & (tb'=2);
[ch4b?] (tb=1) & (execb=false) & (selb=4) -> AVB_RS4:(av4b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS4:(av4b'=false) & (execb'=false) & (tb'=2);
[ch5b?] (tb=1) & (execb=false) & (selb=5) -> AVB_RS5:(av5b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS5:(av5b'=false) & (execb'=false) & (tb'=2);
[ch6b?] (tb=1) & (execb=false) & (selb=6) -> AVB_RS6:(av6b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS6:(av6b'=false) & (execb'=false) & (tb'=2);
[ch7b?] (tb=1) & (execb=false) & (selb=7) -> AVB_RS7:(av7b'=true) & (execb'=true) & (tb'=2) + 1-AVB_RS7:(av7b'=false) & (execb'=false) & (tb'=2);

//start the move
[be?] (tb=1) -> (tb'=2);
//[end?] (tb=2) -> (tb'=2);

//schedule the turn between subsystems
[] (tb=2) -> (tb'=0);

endmodule


